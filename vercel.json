{
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, immutable"
        }
      ]
    },
    {
      "source": "/image/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000"
        }
      ]
    },
    {
      "source": "/asset/reward/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000"
        }
      ]
    },
    {
      "source": "/guideImage/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000"
        }
      ]
    },
    {
      "source": "/icon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000"
        }
      ]
    },
    {
      "source": "/logo/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/raidguide/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=60, s-maxage=31536000, stale-while-revalidate=3600"
        }
      ]
    }
  ],

  "crons": [
    {
      "path": "/api/cron",
      "schedule": "00 21 * * *"
    },
    {
      "path": "/api/cron2",
      "schedule": "00 21 * * 2"
    }
  ]
}
