{
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=86400"
        }
      ]
    }
  ],

  "crons": [
    {
      "path": "/api/cron",
      "schedule": "00 20 * * *"
    },
    {
      "path": "/api/cron2",
      "schedule": "00 20 * * 2"
    }
  ]
}
